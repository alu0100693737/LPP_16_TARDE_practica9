module Prct06
  class Lista_enlazada
    
    attr_accessor :cabeza, :ultelem
    
    # create a Struct with :value and :next
    
    def initialize(cabeza1, ultelem1)
   	@cabeza=cabeza1
	@ultelem=ultelem1
    end
    
    def push(args)
      array=[]
      array.unshift << args
    end 
    
    def pop()
      array.shift >> args
    end
    
     def to_tex
    end
    
  end
end
    
